#include <iostream>
#include <fstream>
using namespace std;
int main()
{
    ifstream fin("file.txt");
    if (fin.is_open())
    {
        int* mass = new int[10];
        int i = 0;
        while (fin >> mass[i])
        {
            i++;
        }
        int m = i;
        int sum = 0;
        for (int j = 0; j < m; j++)
        {
            cout << mass[j] << " ";
            sum += mass[j];
        }
        cout << endl;
        cout << sum << endl;
        fin.close();
    }
    else
        cout << "File ne naiden." << endl;
}

#include <iostream>
using namespace std;

int sign(int b)
{
	if (b > 0)
	{
		return 1;
	}
	if (b==0)
	{
		return 0;
	}
	if (b<0)
	{
		return -1;
	}
}

int main()
{
	int a;
	cin >> a;
	cout << sign(a) << endl;
}



#include <iostream>
#include <fstream>

int spryam(float a, float b)
{
	return (a * b);
}

float streug(int a, int h)
{
	return (a * h / 2);
}

double skr(double r)
{
	return r * r * 3.14;
}

using namespace std;
int main()
{
	cout << spryam(5, 10) << endl;
	cout << streug(5, 10) << endl;
	cout << skr(2.3) << endl;
}



#include <iostream>
#include <fstream>


using namespace std;
int main()
{
	for (int i = 0; i < 40; i++)
	{
		cout << "-";
	}
	cout << endl;
	for (int i = 0; i < 6; i++)
	{
		for (int j = 0; j < 8; j++)
		{
			cout << "*";
		}
		for (int j = 0; j < 40-8; j++)
		{
			cout << "-";
		}
		cout << endl;
	}
	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j < 40; j++)
		{
			cout << "-";
		}
		cout << endl;
	}

}


#include <iostream>
#include <cmath>
#include <Windows.h>

using namespace std;

int main()
{
    HANDLE handle = GetStdHandle(STD_OUTPUT_HANDLE);
    CONSOLE_SCREEN_BUFFER_INFO consoleInfo;
    GetConsoleScreenBufferInfo(handle, &consoleInfo);
    int height = consoleInfo.srWindow.Bottom - consoleInfo.srWindow.Top;
    int width = consoleInfo.srWindow.Right - consoleInfo.srWindow.Left;

    _COORD c;
    for (double i = 0; i < 7; i += 0.02)
    {
        c.X = (i / 7 * width);
        c.Y = ((-sin(i) / 1 + 1) * (height / 2));
        SetConsoleCursorPosition(handle, c);
        cout << '*';
    }

    int a;
    cin >> a;

    return 0;
}


#include <iostream>
#include <cmath>
#include <Windows.h>

using namespace std;

int ranf(int c)
{
	if (c == 0)
	{
		return 0;
	}
	return (37 * ranf(c - 1) + 3) % 64;
}

int rant(int c)
{
	if (c == 0)
	{
		return 0;
	}
	return (25173 * rant(c - 1) + 13849) % 65537;
}


int main()
{
	for (int i = 1; i < 10; i++)
	{
		cout << rant(i) << endl;
	}
}
